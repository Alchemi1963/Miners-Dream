package com.lavaingot.minersdream.objects.tileentities;

import com.lavaingot.minersdream.init.TileInit;

import net.minecraft.block.BlockContainer;
import net.minecraft.block.material.Material;
import net.minecraft.tileentity.TileEntity;
import net.minecraft.world.World;
import net.minecraftforge.fml.common.registry.GameRegistry;

public class Supertorch extends TileEntityBase{

	protected Supertorch(String uName, Material matter) {
		String uName = "supertorch";
		super(uName, Material.WOOD);
		setUnlocalizedName(uName);
		setHardness(1.0F);
		setResistance(6.0F);
		
	}

}
